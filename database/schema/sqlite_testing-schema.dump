PRAGMA synchronous = OFF;
PRAGMA journal_mode = MEMORY;
BEGIN TRANSACTION;
CREATE TABLE "address_types" (
  "id" bigint(20) ,
  "address_type" varchar(100) NOT NULL,
  PRIMARY KEY ("id")
);
CREATE TABLE "addresses" (
  "id" bigint(20)  DEFAULT NULL,
  "address_1" varchar(100) NOT NULL,
  "address_2" varchar(100) DEFAULT NULL,
  "address_3" varchar(100) DEFAULT NULL,
  "city" varchar(100) NOT NULL,
  "state" varchar(2) NOT NULL,
  "zip" varchar(20) NOT NULL
  
);
CREATE TABLE "bondeds" (
  "id" bigint(20) ,
  "coven_id" bigint(20)  NOT NULL,
  "bonded_date" date NOT NULL,
  "member_id" bigint(20)  NOT NULL,
  PRIMARY KEY ("id")
  
  
);
CREATE TABLE "covens" (
  "id" bigint(20) ,
  "name" varchar(100) NOT NULL,
  "abbreviation" varchar(10) NOT NULL,
  "wheel" varchar(20) DEFAULT NULL,
  "element" varchar(20) DEFAULT NULL,
  PRIMARY KEY ("id")
  
  
);
CREATE TABLE "degrees" (
  "id" bigint(20) ,
  "degree" varchar(20) NOT NULL,
  "initiation_date" date DEFAULT NULL,
  PRIMARY KEY ("id")
);
CREATE TABLE "elements" (
  "element" varchar(20) DEFAULT NULL,
  "tool" varchar(20) DEFAULT NULL
);
CREATE TABLE "emails" (
  "id" int(10) ,
  "member_id" bigint(20)  DEFAULT NULL,
  "is_primary" tinyint(4) DEFAULT 1,
  "email" varchar(255) DEFAULT NULL,
  "description" varchar(100) DEFAULT NULL,
  PRIMARY KEY ("id")
  
);
CREATE TABLE "leaders" (
  "id" bigint(20) ,
  "member_id" bigint(20)  NOT NULL,
  "role_name" varchar(20) NOT NULL,
  "leadership_date" date DEFAULT NULL,
  PRIMARY KEY ("id")
  
);
CREATE TABLE "leadership_roles" (
  "id" bigint(20) ,
  "role_name" varchar(30) DEFAULT NULL,
  "abbreviation" varchar(20) NOT NULL,
  "group_name" varchar(20) NOT NULL,
  "level" varchar(20) DEFAULT NULL,
  PRIMARY KEY ("id")
);
CREATE TABLE "members" (
  "id" bigint(20) ,
  "active" tinyint(4) NOT NULL,
  "user_id" int(11) DEFAULT 0,
  "prefix" varchar(20) DEFAULT NULL,
  "first_name" varchar(50) NOT NULL,
  "middle_name" varchar(50) DEFAULT NULL,
  "last_name" varchar(50) NOT NULL,
  "suffix" varchar(20) DEFAULT NULL,
  "magickal_name" varchar(100) DEFAULT NULL,
  "member_since_date" date DEFAULT NULL,
  "member_end_date" date DEFAULT NULL,
  "date_of_birth" date DEFAULT NULL,
  "time_of_birth" time DEFAULT NULL,
  "place_of_birth" varchar(50) DEFAULT NULL,
  "created_at" timestamp NULL DEFAULT NULL,
  "updated_at" timestamp NULL DEFAULT NULL,
  PRIMARY KEY ("id")
);
CREATE TABLE "members_addresses" (
  "id" bigint(20) ,
  "member_id" bigint(20)  NOT NULL,
  "address_id" bigint(20)  NOT NULL,
  "address_type_id" bigint(20)  DEFAULT NULL,
  PRIMARY KEY ("id")
  
  
  
);
CREATE TABLE "members_covens" (
  "id" bigint(20) ,
  "member_id" bigint(20)  NOT NULL,
  "coven_id" bigint(20)  NOT NULL,
  PRIMARY KEY ("id")
  
  
);
CREATE TABLE "members_degrees" (
  "id" bigint(20) ,
  "member_id" bigint(20)  NOT NULL,
  "degree_id" bigint(20)  NOT NULL,
  PRIMARY KEY ("id")
  
  
);
CREATE TABLE "members_leaders" (
  "id" bigint(20) ,
  "member_id" bigint(20)  NOT NULL,
  "leader_id" bigint(20)  NOT NULL,
  PRIMARY KEY ("id")
  
  
);
CREATE TABLE "members_notes" (
  "id" bigint(20) ,
  "member_id" bigint(20)  NOT NULL,
  "note_id" bigint(20)  NOT NULL,
  PRIMARY KEY ("id")
  
  
);
CREATE TABLE "members_users" (
  "id" int(11) NOT NULL ,
  "member_id" bigint(20)  DEFAULT NULL,
  "user_id" bigint(20)  DEFAULT NULL,
  PRIMARY KEY ("id")
  
  
);
CREATE TABLE "migrations" (
  "id" int(10) ,
  "migration" varchar(255) NOT NULL,
  "batch" int(11) NOT NULL,
  PRIMARY KEY ("id")
);
CREATE TABLE "model_has_permissions" (
  "permission_id" bigint(20)  NOT NULL,
  "model_type" varchar(255) NOT NULL,
  "model_id" bigint(20)  NOT NULL,
  PRIMARY KEY ("permission_id","model_id","model_type")
  
);
CREATE TABLE "model_has_roles" (
  "role_id" bigint(20)  NOT NULL,
  "model_type" varchar(255) NOT NULL,
  "model_id" bigint(20)  NOT NULL,
  PRIMARY KEY ("role_id","model_id","model_type")
  
);
CREATE TABLE "notes" (
  "id" bigint(20) ,
  "note" text DEFAULT NULL,
  PRIMARY KEY ("id")
);
CREATE TABLE "password_resets" (
  "email" varchar(255) NOT NULL,
  "token" varchar(255) NOT NULL,
  "created_at" timestamp NULL DEFAULT NULL
);
CREATE TABLE "permissions" (
  "id" bigint(20) ,
  "name" varchar(255) NOT NULL,
  "guard_name" varchar(255) NOT NULL,
  "created_at" timestamp NULL DEFAULT NULL,
  "updated_at" timestamp NULL DEFAULT NULL,
  PRIMARY KEY ("id")
);
CREATE TABLE "role_has_permissions" (
  "permission_id" bigint(20)  NOT NULL,
  "role_id" bigint(20)  NOT NULL,
  PRIMARY KEY ("permission_id","role_id")
  
  
);
CREATE TABLE "roles" (
  "id" bigint(20) ,
  "name" varchar(255) NOT NULL,
  "guard_name" varchar(255) NOT NULL,
  "created_at" timestamp NULL DEFAULT NULL,
  "updated_at" timestamp NULL DEFAULT NULL,
  PRIMARY KEY ("id")
);
CREATE TABLE "security_questions" (
  "id" bigint(20) ,
  "question" varchar(50) DEFAULT NULL,
  PRIMARY KEY ("id")
);
CREATE TABLE "states" (
  "id" bigint(20) ,
  "abbreviation" varchar(2) NOT NULL,
  "name" varchar(100) DEFAULT NULL,
  "country" varchar(2) DEFAULT 'US',
  "is_local" tinyint(4) DEFAULT NULL,
  PRIMARY KEY ("id")
);
CREATE TABLE "users" (
  "id" bigint(20) ,
  "name" varchar(255) NOT NULL,
  "email" varchar(255) NOT NULL,
  "email_verified_at" timestamp NULL DEFAULT NULL,
  "password" varchar(255) NOT NULL,
  "remember_token" varchar(100) DEFAULT NULL,
  "created_at" timestamp NULL DEFAULT NULL,
  "updated_at" timestamp NULL DEFAULT NULL,
  PRIMARY KEY ("id")
);
CREATE TABLE "users_security_questions" (
  "id" bigint(20) ,
  "user_id" bigint(20)  NOT NULL,
  "question_id" bigint(20)  NOT NULL,
  PRIMARY KEY ("id")
  
  
);
CREATE TABLE "wheels" (
  "wheel" varchar(20) DEFAULT NULL
);
CREATE INDEX "states_unq_states_abbreviation" ON "states" ("abbreviation");
CREATE INDEX "members_leaders_fk_members_leaders_members" ON "members_leaders" ("member_id");
CREATE INDEX "members_leaders_fk_members_leaders_leaders" ON "members_leaders" ("leader_id");
CREATE INDEX "covens_fk_covens_elements" ON "covens" ("element");
CREATE INDEX "covens_fk_covens_wheels" ON "covens" ("wheel");
CREATE INDEX "users_security_questions_fk_users_security_questions_security_questions" ON "users_security_questions" ("question_id");
CREATE INDEX "users_security_questions_fk_users_security_questions_users" ON "users_security_questions" ("user_id");
CREATE INDEX "address_types_unq_address_types_address_type" ON "address_types" ("address_type");
CREATE INDEX "model_has_permissions_model_has_permissions_model_id_model_type_index" ON "model_has_permissions" ("model_id","model_type");
CREATE INDEX "leadership_roles_fk_leadership_roles_leaders" ON "leadership_roles" ("role_name");
CREATE INDEX "wheels_unq_wheels_name" ON "wheels" ("wheel");
CREATE INDEX "members_covens_fk_members_covens_members" ON "members_covens" ("member_id");
CREATE INDEX "members_covens_fk_members_covens_covens" ON "members_covens" ("coven_id");
CREATE INDEX "bondeds_fk_bondeds_covens" ON "bondeds" ("coven_id");
CREATE INDEX "bondeds_fk_bondeds_members" ON "bondeds" ("member_id");
CREATE INDEX "model_has_roles_model_has_roles_model_id_model_type_index" ON "model_has_roles" ("model_id","model_type");
CREATE INDEX "users_users_email_unique" ON "users" ("email");
CREATE INDEX "members_addresses_unq_members_addresses_address_id" ON "members_addresses" ("address_id");
CREATE INDEX "members_addresses_unq_members_addresses_member_id" ON "members_addresses" ("member_id");
CREATE INDEX "members_addresses_unq_members_addresses_address_type_id" ON "members_addresses" ("address_type_id");
CREATE INDEX "addresses_unq_addresses_id" ON "addresses" ("id");
CREATE INDEX "addresses_fk_addresses_states" ON "addresses" ("state");
CREATE INDEX "emails_fk_emails_members" ON "emails" ("member_id");
CREATE INDEX "elements_unq_elements_element" ON "elements" ("element");
CREATE INDEX "leaders_unq_leaders_role_name" ON "leaders" ("role_name");
CREATE INDEX "leaders_fk_leaders_members" ON "leaders" ("member_id");
CREATE INDEX "members_degrees_unq_members_degrees_member_id" ON "members_degrees" ("member_id");
CREATE INDEX "members_degrees_fk_members_degrees_degrees" ON "members_degrees" ("degree_id");
CREATE INDEX "role_has_permissions_role_has_permissions_role_id_foreign" ON "role_has_permissions" ("role_id");
CREATE INDEX "password_resets_password_resets_email_index" ON "password_resets" ("email");
CREATE INDEX "members_users_unq_members_users_member_id" ON "members_users" ("member_id");
CREATE INDEX "members_users_unq_members_users_user_id" ON "members_users" ("user_id");
CREATE INDEX "members_notes_fk_members_notes_members" ON "members_notes" ("member_id");
CREATE INDEX "members_notes_fk_members_notes_notes" ON "members_notes" ("note_id");
END TRANSACTION;
